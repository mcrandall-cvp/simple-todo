# Environment Configuration for Simple Todo Backend
# Copy this file to .env for local development
# Docker Compose will override these with container-specific values

# Database connection string (for local development without Docker)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/simple_todo"

# Frontend URL for CORS (for local development without Docker)
FRONTEND_URL="http://localhost:3000"

# Port for the backend server (default: 3001)
PORT=3001
